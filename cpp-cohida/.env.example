# Coinbase API Configuration
# Copy this file to .env and fill in your actual API credentials

# Coinbase Advanced API Credentials
COINBASE_API_KEY=your_api_key_here
COINBASE_API_SECRET=your_api_secret_here
COINBASE_API_PASSPHRASE=your_passphrase_here

# Set to true for sandbox mode (testing)
COINBASE_SANDBOX=false

# Database Configuration
DB_HOST=localhost
DB_PORT=5432
DB_NAME=coinbase_data
DB_USER=your_db_user
DB_PASSWORD=your_db_password
DB_SCHEMA=public
DB_TABLE=historical_data
GRANULARITY_TABLE_SUFFIX=true

# Logging Configuration
LOG_LEVEL=info
LOG_FORMAT=text

# Output Configuration
OUTPUT_DIR=outputs